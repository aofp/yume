{
  "name": "yurucode",
  "version": "1.0.0",
  "description": "Minimal Claude Code SDK IDE",
  "type": "module",
  "author": "Claude Code Studio Team",
  "scripts": {
    "dev": "vite",
    "server:macos": "node --expose-gc --max-old-space-size=2048 server-claude-macos.js",
    "server:multi": "node server-claude-multi.js",
    "build": "vite build && node -e \"const fs=require('fs'); const file='dist/assets/markdown-CK_bGesl.js'; if(fs.existsSync(file)) { const content=fs.readFileSync(file,'utf8'); fs.writeFileSync(file, content.replace(/require_core\\(\\)\\.Object\\.assign/g, 'Object.assign')); console.log('Patched markdown bundle'); }\"",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:mac": "tauri build --target universal-apple-darwin",
    "tauri:build:win": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu"
  },
  "keywords": [
    "claude",
    "ai",
    "sdk",
    "tauri",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@tauri-apps/api": "^2.0.1",
    "@tauri-apps/cli": "^2.0.0-rc.18",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^7.1.1"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.34.1",
    "@tauri-apps/plugin-dialog": "^2.3.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "portfinder": "^1.0.37",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^5.8.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zustand": "^4.4.7"
  }
}
